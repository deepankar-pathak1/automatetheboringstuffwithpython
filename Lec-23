 # we can also add two list with the help of + or wit concatenate
 
 fruit_2 = ["Mango","Orange"]

fruit.extend(fruit_2) #extend will add element to another element in same list
print(fruit)
['apple', 'Grapes', 'Mango', 'Orange']



# Deelet Data From list
lst = ['apple', 'Grapes', 'Mango', 'Orange']
# lst.pop() # POP will remove last Element from the list
# print(lst)
lst.pop(2) # delete element from index 2
print(lst) # mango will remove from the list


# del lst[1] # delete from index 1
print(lst)


# remove

lst.remove('apple')
print(lst)
