# practice of function

# here we want to print last char of string.
name = input("Name : ")
def lst_char(x):

    return x[-1]
print(lst_char(name))





x = int(input("Number : "))

def odd_even(x):
    if x % 2 == 0:
        print(f"Even Number {x}")
    else:
        print(f"Odd Number {x}")

odd_even(x)




# in short Odd-Even

x = int(input("Number : "))
def odd_even(x):
    if x % 2 == 0:
        return "Even"
    return "Odd"

print(odd_even(x))




# # Return Me a boolean value if Its Even
