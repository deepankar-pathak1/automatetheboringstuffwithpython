# def even(num):
#     if num % 2 == 0:
#         return True
#     return False
# 
# print(even(6))




# 
# def is_even(num):
#     return num % 2 == 0 # Return True if even
# 
# print(is_even(6))




x = int(input("Number 1 : "))
y = int(input("Number 2 : "))

def greater(a,b):
    if a > b:
        return a
    elif b > a:
        return b
    else:
        print("Valid Number ")

print(f"Greater {greater(x,y)}")

